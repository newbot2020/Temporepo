@model IEnumerable<one2many.Models.Result>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.term)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.termresult)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.meritpostion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Students)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Classinfo)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.term)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.termresult)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.meritpostion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Students.email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Classinfo.id)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
